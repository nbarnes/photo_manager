<%= render 'partials/header', page_header_title: "Photos" %>

<% @photos.each do |photo| %>

    <%= render "partials/large_photo", photo: photo %>

    <%= link_to 'Show', photo %>
    <%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %>
    <br /><br />
<% end %>
<%= link_to 'New Photo', new_photo_path %>
